{
  "name": "high-five",
  "version": "0.1.0",
  "description": "Modular visual control system",
  "repository": {
    "type": "git",
    "url": "git@github.com:Scoreaux/high-five.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "start": "npm run build:dev && node ./dist/app/index.js",
    "start:app": "npm run build:app:dev && node ./dist/app/index.js",
    "start:client": "webpack-dev-server --mode development --config ./webpack.client.config.js",
    "build:dev": "npm run clean && npm run build:app:dev && npm run build:client:dev",
    "build:prod": "npm run clean && npm run build:app:prod && npm run build:client:prod",
    "build:app:dev": "webpack --mode development --config ./webpack.app.config.js",
    "build:app:prod": "webpack --mode production --config ./webpack.app.config.js",
    "build:client:dev": "webpack --mode development --config ./webpack.client.config.js",
    "build:client:prod": "webpack --mode production --config ./webpack.client.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.2",
    "postcss-atrule-bem": "^3.1.1",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "resolve-url-loader": "^2.3.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-app": "^3.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.0-beta.6",
    "apollo-client": "^2.3.7",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-app-koa": "^2.0.0",
    "apollo-upload-client": "^8.1.0",
    "axios": "^0.18.0",
    "client-oauth2": "^4.2.1",
    "conf": "^2.0.0",
    "electron-store": "^2.0.0",
    "env-paths": "^1.0.0",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "koa": "^2.5.2",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.10",
    "node-watch": "^0.5.8",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "triple-beam": "^1.3.0",
    "winston": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(gif|png|jpe?g|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(json|geojson|)$": "<rootDir>/__mocks__/jsonMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^app(.*)$": "<rootDir>/src/app$1",
      "^client(.*)$": "<rootDir>/src/client$1"
    }
  }
}
